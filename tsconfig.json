{
  "compilerOptions": {
    "target": "es6",
    "jsx": "react",
    "typeRoots": ["./node_modules/@types", "./node_modules/@figma"],
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/lib/**/*.ts",
    "src/frames/**/*.ts",
  ]
}
